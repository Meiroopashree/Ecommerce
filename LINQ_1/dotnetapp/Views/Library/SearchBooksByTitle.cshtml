@model List<dotnetapp.Models.Book>

<h2>Search Results</h2>

@if (Model.Count == 0)
{
    <p>No books found matching the query.</p>
}
else
{
    <ul>
        @foreach (var book in Model)
        {
            <li>ID: @book.Id</li>
            <li>Title: @book.Title</li>
            <li>Author: @book.Author</li>
            <li>Published Year: @book.PublishedYear</li>
        }
    </ul>
}
